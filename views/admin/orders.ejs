    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const statusFilter = document.getElementById('statusFilter');
            const tableBody = document.querySelector('tbody');

            statusFilter.addEventListener('change', function () {
                const selectedStatus = this.value;
                filterOrders(selectedStatus);
            });

            function filterOrders(status) {
                const rows = tableBody.querySelectorAll('tr');
                let ordersFound = false;

                rows.forEach(row => {
                    const orderStatus = row.querySelector('td:nth-child(7) .badge').innerText.trim();
                    if (status === 'All Orders' || orderStatus === status) {
                        row.style.display = '';
                        ordersFound = true;
                    } else {
                        row.style.display = 'none';
                    }
                });
                const noOrdersMessage = tableBody.querySelector('.no-orders-message');
                if (noOrdersMessage) {
                    noOrdersMessage.remove();
                }
                if (!ordersFound) {
                    const noOrdersRow = document.createElement('tr');
                    noOrdersRow.classList.add('no-orders-message');
                    noOrdersRow.innerHTML = '<td colspan="8" class="text-center">No orders with this status</td>';
                    tableBody.appendChild(noOrdersRow);
                }
            }
        });



        document.addEventListener('DOMContentLoaded', function () {
    document.querySelectorAll('.dropdown-menu .dropdown-item').forEach(item => {
        item.addEventListener('click', function (event) {
            event.preventDefault();

            const orderId = this.closest('.dropdown-menu').getAttribute('data-order-id');
            const newStatus = this.getAttribute('data-status');

            if (newStatus === 'Completed') {
                // Disable further changes if status is Completed
                this.closest('.dropdown-menu').querySelectorAll('.dropdown-item').forEach(option => {
                    option.classList.add('disabled');
                });
            }

            updateOrderStatus(orderId, newStatus);
        });
    });

    function updateOrderStatus(orderId, newStatus) {
        fetch(`/admin/updateOrderStatus`, {
            method: 'PATCH',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ orderId, newStatus })
        })
        .then(response => response.json())
        .then(data => {
            if (data.success) {
                // Update the status on the frontend
                const orderRow = document.querySelector(`[data-order-id="${orderId}"]`).closest('tr');
                orderRow.querySelector('td:nth-child(7) .badge').innerText = newStatus;

                if (newStatus === 'Completed') {
                    // Disable dropdown for completed orders
                    orderRow.querySelector('.dropdown-menu').style.pointerEvents = 'none';
                    orderRow.querySelector('.dropdown-toggle').classList.add('disabled');
                }
            } else {
                alert('Failed to update order status');
            }
        })
        .catch(error => console.error('Error:', error));
    }
});
    </script>



