  <script>
    function navigateToEdit(categoryId) {
      window.location.href = `/admin/EditCategories/${categoryId}`;
    }

    function toggleBlockStatus(categoryId, btnElement, statusElement) {
      const confirmationMessage = btnElement.innerText.trim() === 'Unlist'
        ? 'Are you sure you want to Unlist this Category?'
        : 'Are you sure you want to List this Category?';

      Swal.fire({
        title: confirmationMessage,
        icon: "warning",
        showCancelButton: true,
        confirmButtonColor: "#3085d6",
        cancelButtonColor: "#d33",
        confirmButtonText: "Yes, Sure!"
      }).then((confirm) => {
        if (confirm) {
          fetch(`/admin/unlist-category/${categoryId}`, {
            method: 'PATCH',
            headers: {
              'Content-Type': 'application/json'
            }
          })
            .then(response => response.json())
            .then((data) => {
              const isActive = btnElement.innerText.trim() === 'Unlist';
              btnElement.innerText = isActive ? 'List' : 'Unlist';
              btnElement.classList.toggle('btn-success', isActive);
              btnElement.classList.toggle('btn-danger', !isActive);

              statusElement.innerText = isActive ? 'Unlisted' : 'Listed';
              statusElement.classList.toggle('badge-success-soft', !isActive);
              statusElement.classList.toggle('badge-danger', isActive);
              statusElement.classList.toggle('text-success', !isActive);
              statusElement.classList.toggle('text-danger', isActive);

            })
        }
      })

    }
  </script>

