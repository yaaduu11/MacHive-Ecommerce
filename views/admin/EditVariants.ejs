                    <div>
                        <!-- row -->

                        <div class="row">
                            <form id="EditVariantForm" enctype="multipart/form-data" class="row"
                                action="/admin/EditVariant/<%= variant._id %>" method="POST">
                                <div class="col-lg-8 col-12 mx-auto">
                                    <div class="card mb-4">
                                        <div class="card-body">
                                            <div>
                                                <br>
                                                <div class="mb-3">
                                                    <label class="form-label">Variant Name</label>
