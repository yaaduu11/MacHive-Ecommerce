    function toggleBlockStatus(couponId, btnElement, statusElement) {
  const isCurrentlyBlocked = btnElement.innerText.trim() === 'Unlist';
  const confirmationMessage = isCurrentlyBlocked 
    ? 'Are you sure you want to Unlist this Coupon?' 
    : 'Are you sure you want to List this Coupon?';

  Swal.fire({
    title: confirmationMessage,
    icon: "warning",
    showCancelButton: true,
    confirmButtonColor: "#3085d6",
    cancelButtonColor: "#d33",
    confirmButtonText: "Yes, Sure!"
  }).then((result) => {
    if (result) {
      fetch(`/admin/unlist-coupon/${couponId}`, {
        method: 'PATCH',
        headers: {
          'Content-Type': 'application/json'
        }
      })
      .then(response => response.json())
       .then((data) => {
        const newBlockStatus = btnElement.innerText.trim() === 'Unlist';
        btnElement.innerText = newBlockStatus ? 'List' : 'Unlist';
        btnElement.classList.toggle('btn-success', newBlockStatus);
        btnElement.classList.toggle('btn-danger', !newBlockStatus);

        statusElement.innerText = newBlockStatus ? 'Listed' : 'Unlisted';
        statusElement.classList.toggle('badge-success-soft', newBlockStatus);
        statusElement.classList.toggle('badge-danger', !newBlockStatus);
        statusElement.classList.toggle('text-success', newBlockStatus);
        statusElement.classList.toggle('text-danger', !newBlockStatus);

      })
    }
  });
}



  </script>

