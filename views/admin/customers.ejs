    <script>
        function toggleBlockStatus(userId, btnElement, statusElement) {
            const confirmationMessage = btnElement.innerText.trim() === 'Block'
                ? 'Are you sure you want to block this user?'
                : 'Are you sure you want to unblock this user?';

            swal({
                title: confirmationMessage,
                icon: "warning",
                buttons: ['Cancel', 'Confirm'],
                confirmButtonColor: "#cc0000",
                cancelButtonColor: "#d33",
                confirmButtonText: "Yes, delete it!"
            }).then((confirm) => {
                if (confirm) {
                    fetch(`/admin/block-user/${userId}`, {
                        method: 'PATCH',
                        headers: {
                            'Content-Type': 'application/json'
                        }
                    })
                        .then(response => response.json())
                        .then(data => {
                            if (data.success) {
                                const isActive = btnElement.innerText.trim() === 'Block';
                                btnElement.innerText = isActive ? 'Unblock' : 'Block';
                                btnElement.classList.toggle('btn-success', isActive);
                                btnElement.classList.toggle('btn-danger', !isActive);

                                statusElement.innerText = isActive ? 'Blocked' : 'Active';
                                statusElement.classList.toggle('badge-success-soft', !isActive);
                                statusElement.classList.toggle('badge-danger', isActive);
                                statusElement.classList.toggle('text-success', !isActive);
                                statusElement.classList.toggle('text-danger', isActive);
                            }
                        })
                        .catch(error => console.error('Error:', error));
                }
            });
        }
    </script>




