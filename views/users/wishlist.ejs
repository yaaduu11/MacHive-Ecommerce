    <main>

        <!-- cart-area start -->
        <section class="cart-area pb-120 pt-120">
            <h3 style="text-align: center; 
                        font-family: 'Lato', sans-serif;
                        font-size: 32px;
                        margin-top: -100px;
                        font-weight: 400;
                        color: #555; 
                        text-align: center;
                        margin: 20px auto;
                        position: relative;
                        margin-top: -50px;
                        cursor: pointer; ">WISHLIST</h3>
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        


                        <div class="table-content table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th class="product-thumbnail">Image</th>
                                        <th class="cart-product-name">Product</th>
                                        <th class="product-price">Price</th>
                                        <th class="product-subtotal"></th>
                                        <th class="product-remove">Remove</th>
                                    </tr>
                                </thead>
                                <tbody>
                                  <% wishlist.products.forEach((item, index) => { %>
                                    <tr>
                                        <td class="product-thumbnail"><a><img
                                                    src="<%= item.productVariantId.productId.image %>" alt="<%= item.productVariantId.productId.name %>"></a></td>
                                        <td class="product-name"><a><%= item.productVariantId.productId.name %></a>
                                        </td>
                                        <td class="product-subtotal"><span class="amount">
                                            <% if (item.productVariantId.is_blocked) { %>
                                                <span class="text-danger">Product is unavailable</span>
                                            <% } else if (item.productVariantId.stock === 0) { %>
                                                <span class="text-danger">This product is out of stock</span>
                                            <% } else { %>
                                                â‚¹ <%= item.productVariantId.salePrice %>
                                            <% } %></span>
                                        </td>
                                        <td class="product-quantity">
                                            <a href="cart.html" class="btn-tp">Add To Cart</a>
                                        </td>
                                        <td class="product-remove"><a href="#"><i class="fa fa-times"></i></a></td>
                                    </tr>
                                  <% }) %>
                                </tbody>
                            </table>
                        </div>

                    </div>
                </div>
            </div>
        </section>
        <!-- cart-area end -->

